{
  "name": "nobator",
  "productName": "NobaTor",
  "version": "1.1.1",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "コカマッシュ",
  "license": "MIT",
  "description": "",
  "config": {
    "forge": "./forge.config.js"
  },
  "build": {
    "appId": "net.cocamush.nobator",
    "productName": "NobaTor",
    "files": [
      "dist/**/*",
      "src/**/*",
      "main.js",
      "package.json"
    ],
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "ccmsh",
        "repo": "NobaTor"
      }
    ],
    "win": {
      "icon": "icon.ico",
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "icon": "icon.icns"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build/icons/installer.ico",
      "uninstallerIcon": "build/icons/uninstaller.ico",
      "uninstallDisplayName": "NobaTor Uninstaller"
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0",
    "@electron-forge/maker-deb": "^6.0.0",
    "@electron-forge/maker-rpm": "^6.0.0",
    "@electron-forge/maker-squirrel": "^6.0.0",
    "@electron-forge/maker-zip": "^6.0.0",
    "@electron-forge/publisher-github": "^6.0.0",
    "@electron/packager": "^18.3.6",
    "electron": "^34.0.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "easymde": "^2.18.1-177.0",
    "electron-prompt": "^1.7.0",
    "electron-updater": "^6.3.9",
    "path": "^0.12.7"
  }
}
