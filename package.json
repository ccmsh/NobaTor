{
  "name": "nobator",
  "productName": "NobaTor",
  "version": "1.2.0",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "build": {
    "asar": {
      "unpack": ".env"
    },
    "appId": "net.cocamush.nobator",
    "productName": "NobaTor",
    "files": [
      "dist/**/*",
      "src/**/*",
      "main.js",
      "preload.js",
      "renderer.js",
      "package.json"
    ],
    "extraFiles": [
      {
        "from": ".env",
        "to": ".env"
      }
    ],
    "directories": {
      "output": "build"
    },
    "mac": {
      "icon": "nobazine.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "nobazine.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@electron-forge/publisher-github": "^7.6.1",
    "@electron/packager": "^18.3.6",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "easymde": "^2.18.1-177.0",
    "electron-prompt": "^1.7.0",
    "path": "^0.12.7"
  }
}